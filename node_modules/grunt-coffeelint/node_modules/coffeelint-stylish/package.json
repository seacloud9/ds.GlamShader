{
  "name": "coffeelint-stylish",
  "version": "0.1.1",
  "description": "Stylish reporter for CoffeeLint",
  "keywords": [
    "reporter",
    "validate",
    "stylish",
    "elegant",
    "pretty",
    "lint",
    "coffee",
    "coffeelint",
    "coffeescript",
    "coffee-script",
    "codeconventions"
  ],
  "repository": {
